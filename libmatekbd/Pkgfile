# Depends on: libxklavier intltool gtk

name=libmatekbd
version=1.8.0
release=1
source=(http://pub.mate-desktop.org/releases/${version%.*}/$name-$version.tar.xz)

build() {
    cd $name-$version
    ./configure --prefix=/usr \
        --disable-nls \
        --disable-schemas-compile
    make
    make DESTDIR=$PKG install
}
